    "antd-mobile": "^5.33.2",
    "antd-mobile-icons": "^0.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^3.1.1",
